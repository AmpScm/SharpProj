    <file src="..\src\SharpProj.NetTopologySuite\bin\release\SharpProj.NetTopologySuite.xml" target="lib/net46" />

    <file src="..\src\SharpProj.NetTopologySuite\bin\releaseCore\SharpProj.NetTopologySuite.dll" target="lib/netcore" />
