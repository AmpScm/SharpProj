<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SharpProj.Core</id>
    <version>7.2001.2</version>
    <authors>SharpSvn</authors>
    <owners>SharpSvn</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <description>SharpProj wraps PROJ for .Net. At runtime usually a 'proj.db' package is required,
    such as provided by the SharpProj.Database nuget package. The version number consists of the PROJ version
    and the wrapping. As Such SharProj 7.2001.3 is based on PROJ 7.2.1 (7.2001) and is SharpProj build 3 for
    this version.
    Please use the SharpProj nuget package unless you explicitly want to ignore the database.</description>
    <dependencies>
      <group targetFramework="net45" />
    </dependencies>
  </metadata>
  <files>
    <file src="..\src\SharpProj\bin\Win32_release\SharpProj.dll" target="lib/net45/win-x86" />
    <file src="..\src\SharpProj\bin\Win32_release\SharpProj.xml" target="lib/net45/win-x86" />
    <file src="..\src\SharpProj\bin\x64_release\SharpProj.dll" target="lib/net45/win-x64" />
    <file src="..\src\SharpProj\bin\x64_release\SharpProj.xml" target="lib/net45/win-x64" />
  </files>
</package>