{
  "name": "proj4",
  "version-string": "8.0.0",
  "description": "PROJ.4 library for cartographic projections",
  "homepage": "https://github.com/OSGeo/PROJ",
  "dependencies": [
    {
      "name": "sqlite3",
      "default-features": false
    },
    "tiff"
  ],
  "default-features": [
    "database"
  ],
  "features": {
    "database": {
      "description": "generate database",
      "dependencies": [
        {
          "name": "sqlite3",
          "host": true,
          "features": [
            "tool"
          ]
        }
      ]
    },
    "tools": {
      "description": "generate tools",
      "dependencies": [
        "curl"
      ]
    }
  }
}
